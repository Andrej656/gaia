{
  "name": "gaia-admin",
  "version": "0.0.1",
  "description": "Service for administrating a Gaia hub",
  "main": "lib/index.js",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "lint": "eslint --ext .ts ./src -f unix",
    "lint:fix": "eslint --ext .ts ./src --fix",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test": "run-p build lint test:coverage",
    "start": "node lib/index.js"
  },
  "bin": {
    "blockstack-gaia-admin": "./cmd/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacks-network/gaia"
  },
  "keywords": [
    "decentralized",
    "storage"
  ],
  "author": "Jude Nelson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stacks-network/gaia"
  },
  "dependencies": {
    "ajv": "^6.10.2",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.5",
    "express": "^4.18.1",
    "express-winston": "^4.2.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-winston": "^4.0.0",
    "@types/jest": "^28.1.3",
    "@types/node": "^18.0.0",
    "@types/tape": "^4.13.2",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.18.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.1",
    "npm-run-all": "^4.1.5",
    "source-map-support": "^0.5.21",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "homepage": "https://github.com/stacks-network/gaia",
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "verbose": true
  },
  "type": "module"
}
